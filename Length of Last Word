Problem Link:-https://leetcode.com/problems/length-of-last-word/submissions/


class Solution {
public:
    int lengthOfLastWord(string s) {
        int count=0;
        for(int i=s.size()-1;i>=0;i--){
             if(s[i]==' ' and count!=0) {
                 break;
             }
            if(s[i]!=' ') {
                count++;
            }
            
            }
           return count;
            
        }
   
    
};
