Problem Link:-https://practice.geeksforgeeks.org/problems/sum-of-elements-in-a-matrix2000/1/?company[]=Zoho&company[]=Zoho&page=1&query=company[]Zohopage1company[]Zoho#



class Solution {
  public:
    int sumOfMatrix(int N, int M, vector<vector<int>> Grid) {
        int sum=0;
        for(int i=0;i<N;i++){
            for(int j=0;j<M;j++){
                sum+=Grid[i][j];
            }
        }
    
        return sum;
    }
};
