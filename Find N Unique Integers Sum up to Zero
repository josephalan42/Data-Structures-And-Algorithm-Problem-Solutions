Problem Link:-https://leetcode.com/problems/find-n-unique-integers-sum-up-to-zero/



class Solution {
public:
    vector<int> sumZero(int n) {
        vector<int>res;
        int ans=0;
        for(int i=1;i<n;i++){
            res.push_back(i);
            ans=ans+i;
        }
     res.push_back(-ans);
   return res; 
    
    }
};
