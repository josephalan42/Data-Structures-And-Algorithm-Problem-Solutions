Problem Link:-https://www.hackerrank.com/challenges/arrays-introduction/problem



#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int N;
    std::cin>>N;
    int A[N];
    for(int i=0;i<N;i++){
        std::cin>>A[i];
    }  
    for(int k=N-1;k>=0;k--){
        std::cout<<A[k]<<" ";
    }
    
    
    return 0;
}
