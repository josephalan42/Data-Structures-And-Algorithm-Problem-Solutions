Problem Link:-https://leetcode.com/problems/convert-binary-number-in-a-linked-list-to-integer/submissions/


class Solution {
public:
    int getDecimalValue(ListNode* head) {
        int Result=0;
        while(head!=NULL){
            Result=Result*2+head->val;
            head=head->next;
        }
    return Result;
    }
};
