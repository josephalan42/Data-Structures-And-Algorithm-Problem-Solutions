Problem Link:-https://leetcode.com/problems/guess-number-higher-or-lower/submissions/



class Solution {
public:
    int guessNumber(int n) {
        int i=1;
        int j=n;
        while(i<=j)
        {
            int mid=i+(j-i)/2;
            int key=guess(mid);
            
            if(key==0)
                return mid;
            else if(key<0)
                j=mid-1;
            else 
                i=mid+1;
        }
        return -1;
        
    }
};
