Problem Link:-https://leetcode.com/problems/container-with-most-water/submissions/



class Solution {
public:
    int maxArea(vector<int>& height) 
    {
        int l=0, 
        int r=height.size()-1
        int res=0;
        while(l<r)
        {
            int minn = min(height[l],height[r]);
            res = max(res,mini*(r-l));
            if(height[l]>height[r])
               r--;
            else
                l++;
        }
        return res;
    }
};
