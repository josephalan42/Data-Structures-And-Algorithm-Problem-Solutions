Problem Link:-https://leetcode.com/problems/sort-array-by-parity/submissions/



class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& nums) {
       vector<int>s;
        for(int i=0;i<nums.size();i++){
           if(nums[i]%2==0){
               s.push_back(nums[i]);
           }
        }
        for(int j=0;j<nums.size();j++){
            if(nums[j]%2!=0){
            s.push_back(nums[j]);
                }
            }
        
         
    return s;
    
    }
};
