Problem Link:-https://www.hackerrank.com/challenges/compare-the-triplets/problem


vector<int> compareTriplets(vector<int> a, vector<int> b) {
    int A=0;
    int B=0;
    vector<int>res;
    for(int i=0;i<a.size();i++){
        if(a[i]>b[i]){
            A++;
        }
        else if(b[i]>a[i]){
            B++;
        }
        
    }
    res.push_back(A);
    res.push_back(B);
    return res;
}
