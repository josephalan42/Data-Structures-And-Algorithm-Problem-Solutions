Problem Link:-https://leetcode.com/problems/search-in-rotated-sorted-array/submissions/



class Solution {
public:
    int search(vector<int>& nums, int target) {
        int ind;
        int count=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==target){
                ind=i;
                count++;
            }
        }
    if(count==0){
        return -1;
    }
        return ind;
    }
};
